ify(this.state.credentials)\n    })\n    .then( data => data.json())\n    .then(\n      data => {\n        this.props.userLogin(data.token);\n      }\n    )\n    .catch( error => console.error(error))\n  }\n\n  render(){\n\n  \n    return (\n      <FormSection>\n      <AppEl>\n        <WelcomeText>Zarejestruj się</WelcomeText>\n        <InputContainer>\n          <Input type=\"text\" placeholder=\"Imię\" />\n          <Input type=\"text\" placeholder=\"Email\" name=\"username\" \n           value={this.state.username}\n           onChange={this.onChange} />\n          <Input type=\"password\" placeholder=\"Hasło\" name=\"password\"\n           value={this.state.password}\n           onChange={this.onChange} />\n        </InputContainer>\n        <ButtonContainer>\n        <Button onClick={this.onLoginClick}>Zarejestruj się</Button>\n        </ButtonContainer>\n      </AppEl>\n      </FormSection>\n    );\n  }\n}\n\nexport default Register;","/Users/aleksandrawerda/Desktop/fiszki-dla-niemych-5/react/src/assets/Pages/Home.js",[],"/Users/aleksandrawerda/Desktop/fiszki-dla-niemych-5/react/src/assets/Pages/CreateCategory.js",[],"/Users/aleksandrawerda/Desktop/fiszki-dla-niemych-5/react/src/assets/Pages/CreateCard.js",[],"/Users/aleksandrawerda/Desktop/fiszki-dla-niemych-5/react/src/assets/Pages/Form.js",["128"],"/Users/aleksandrawerda/Desktop/fiszki-dla-niemych-5/react/src/assets/Theme.js",[],"/Users/aleksandrawerda/Desktop/fiszki-dla-niemych-5/react/src/assets/components/styled/Button.styled.js",[],"/Users/aleksandrawerda/Desktop/fiszki-dla-niemych-5/react/src/assets/components/Hero/Hero.js",["129"],"/Users/aleksandrawerda/Desktop/fiszki-dla-niemych-5/react/src/assets/components/styled/H3.styled.js",[],"/Users/aleksandrawerda/Desktop/fiszki-dla-niemych-5/react/src/assets/components/Inputs/Input.js",["130","131"],"import styled from \"styled-components\";\nimport { Colors, TextStyles } from \"../../Theme\";\nimport { Shadows } from \"../../Theme\";\nexport default functi